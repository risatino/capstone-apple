<section>
<h2><%= link_to 'Back To Home Page', '/profiles', class: "btn btn-info" %> <%= link_to 'New', '/profiles', method: :post class: "btn btn-info" %> </h2>
<h2>Showing All User Profiles</h2>

<% @profiles.each do |profile| %>
  <h2>Company Address: <%= profile.address %></h2>
  <h2>Phone Number: <%= profile.phone %></h2>
  <h2>Description: <%= profile.description %></h2>
  <h2>Active Job Tickets: <%= profile.active_ticket %></h2>
<% end %>
</section>
